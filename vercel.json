{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "",
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/", "destination": "/prototype/multi-region-art/index.html" },
    { "source": "/app", "destination": "/prototype/multi-region-art/index.html" },
    { "source": "/app/(.*)", "destination": "/prototype/multi-region-art/$1" },
    { "source": "/landing", "destination": "/docs/index.html" },
    { "source": "/guide", "destination": "/docs/guide.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
